general {
	output_format = "i3bar"
	colors = false
	markup = pango
	interval = 5
}

order += "volume master"
order += "time"



volume master {
	format = "<span></span><span>%volume</span>"
	format_muted = "<span></span><span>Muted</span>"
	device = "default"
	mixer = "Master"
	mixer_idx = 0
}



time {
	format = "<span></span><span>%Y-%m-%d %H:%M:%S</span>"
}

